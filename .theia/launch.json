  "version": "0.2.0",
  "configurations": [
      {
          "type": "java",
          "name": "Debug (Launch) - Current File",
          "request": "launch",
          "mainClass": "${file}"
      },
      {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole"
        }
  ]
}
